<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:tns="http://larionov.com/epam-spring-boot-soap"
           targetNamespace="http://larionov.com/epam-spring-boot-soap"
           elementFormDefault="qualified">

    <xs:element name="GetBrandRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="id" type="xs:long"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="GetBrandResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="brand" type="tns:Brand"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:complexType name="Brand">
        <xs:sequence>
            <xs:element name="id" type="xs:long"/>
            <xs:element name="name" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>


</xs:schema>